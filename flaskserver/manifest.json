{
	"manifest_version": 2, 
	"name": "NASA", 
	"description": "Sees if an article might be clickbait.",
	"version": "1",
	"author": "TheNASA",

  "page_action": {
    "default_icon": "1200px-NASA_logo.svg.png",
    "default_title": "Clickbait",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts":  ["background.js"],
    "persistent": false
  },
	
	"chrome_url_overrides" : {
		"newtab": "newtab.html"
	},
	
	"permissions": ["activeTab"]}
	
}